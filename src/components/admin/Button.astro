---
const { dataPage, text } = Astro.props;
---
<style>
    .admin-button {
        padding: 10px 20px;
        background-color: #111827;
        color: rgb(255, 255, 255);
        border: none;
        cursor: pointer;
        font-size: 16px;
    }
    
    .admin-button:hover {
        color: rgb(42, 85, 204);
        background-color: #1c298b;
    }
</style>
<button class="admin-button" data-page={dataPage}>
    {text}
</button>